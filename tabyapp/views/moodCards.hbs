 <section id="features" class="service-one">
            <div class="container">
                <div class="block-title text-center">
                    <p>Track your Moods {{name}}</p>
                    <h3>DonÂ´t forget to share your Mood</h3>
                </div>
                <div class="row">
                    {{#each user.moods}}
                    <div class=" col-xl-3 col-lg-3 col-md-6 wow fadeInLeft" data-wow-duration="1500ms">
                        <div class="service-one__single">
                            <div class="service-icon">
                                <ul class="footer-widget__social">
                                <li><a href="/mood/update/{{_id}}" class="color-1"><i class="fas fa-edit"></i></a></li>
                                <li><a href="/mood/delete/{{_id}}"class="color-2"><i class="fas fa-minus-circle"></i></a></li>
                            </ul>
                                <div class="icon-box" style="background-color: aqua;">
                                </div>
                            </div>
                            <div class="text">
                                <h3>I have felt {{mood}}</h3>
                                <p>{{note}}</p>
                            </div>
                            <br>
                            <ul class="list-unstyled pricing-one__list">
                                <li><i></i>In a scale of: {{scale}}</li>
                            </ul>
                            <a href="/mood/{{_id}}" class="thm-btn pricing-one__btn"><span>Go details</span></a>
                            <br>
                            <br> 
